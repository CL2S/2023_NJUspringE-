ags: [{ name: \"name1\", nick: \"标签一\" }],\n      inputVisible: false,\n      inputValue: \"\"\n    };\n  },\n  methods: {\n    handleClose(tag) {\n      this.dynamicTags.splice(this.dynamicTags.indexOf(tag), 1);\n    },\n\n    showInput() {\n      this.inputVisible = true;\n      this.$nextTick(_ => {\n        this.$refs.saveTagInput.$refs.input.focus();\n      });\n    },\n    add(data) {\n      if (data) {\n        this.dynamicTags.push(data);\n      }\n\n      this.inputVisible = false;\n      this.inputValue = \"\";\n    },\n    handleBlur() {\n      this.inputVisible = false;\n      this.inputValue = \"\";\n    },\n    handleInputConfirm() {\n      this.add(this.inputValue);\n    },\n    handleSelect(item) {\n      console.log(item);\n      this.add(item);\n    },\n    querySearchAsync(queryString, cb) {\n      if (!queryString) {\n        return;\n      }\n\n      this.ajax\n        .get(this.url, {\n          params: {\n            keyword: queryString\n          }\n        })\n        .then(result => {\n          if (result.code == 0) {\n            cb(result.data);\n          } else {\n            this.error(result.msg);\n          }\n        });\n    }\n  }\n};\n</script>"],"sourceRoot":"src\\commons"}]}